on init
    exec u:r:vendor_init-qti-keymaster-sh:s0 -- /vendor/bin/init.qti.keymaster.sh

on property:vendor.keymaster.strongbox.version=40
    enable keymaster-sb-4-0
    start keymaster-sb-4-0

service keymaster-sb-4-0 /vendor/bin/hw/android.hardware.keymaster@4.0-strongbox-service-qti
    class early_hal
    user system
    group system drmrpc
